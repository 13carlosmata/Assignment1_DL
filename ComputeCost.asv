function J = ComputeCost(X, Y, W, b, lambda)
K=10;
P=[];
n=size(X,2);
D=n;
lcross=0;
tr1=(Y)';
for i=1:n
%   for j=1:K
%       if tr1(i,j)==1
%           index=j;
%       end
%  end
  xp1=X(:,i);
  s =W*xp1 + b;
  P1=softmax(s);
  P=[P,P1];
  
  lcross=-log(tr1(i,:)*P1(:,i))+lcross;
end
J1=lcross/n;
sumW=sum(W(:));
J2=lambda*sum(W^2);
J=J1+J2;
end